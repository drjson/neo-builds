auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp
	pre-up /etc/network/nfs_check
        hostname udoo-neo
	wait-delay 15

allow-hotplug wlan0
iface wlan0 inet dhcp
	pre-up wpa_supplicant -B -i wlan0 -c /etc/wpa_supplicant.conf
	post-down killall -q wpa_supplicant
	wait-delay 30

